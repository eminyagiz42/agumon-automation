<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>standard</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Spark Extent</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Agumon Test Automation Spark Report</reportName>
		
		<!-- view report without internet -->
		<offlineMode>true</offlineMode>
		
		 <thumbnailForBase64>true</thumbnailForBase64>

		<!-- custom javascript -->
		<scripts>
			<![CDATA[
        $(document).ready(function() {

        });
      ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[
			<img src='src/main/resources/extent-config/digi-logo.png'/>
      ]]>
		</styles>

	</configuration>
</extentreports>